# Azure AI Foundry Settings (required for Bing integration)
PROJECT_CONNECTION_STRING=your_connection_string
MODEL_DEPLOYMENT_NAME=your_model_deployment
BING_CONNECTION_NAME=your_bing_connection

# DeepSeek Settings (optional, for enhancement)
AZURE_INFERENCE_ENDPOINT=https://your-endpoint.openai.azure.com/
AZURE_INFERENCE_KEY=your_key
MODEL_NAMEDS=DeepSeek-R1

# Configuration Options
FINOPS_ENHANCEMENT_ENABLED=true
FINOPS_DEBUG_MODE=false

# Azure OpenAI/Inference Settings
SERVERLESS_MODEL_NAME=gpt-4o
MODEL_NAME=gpt-4o

# Frontend Settings
VITE_API_URL=https://localhost:8003
VITE_DEFAULT_THEME=dark
VITE_API_VERSION=v1

# Azure OpenAI Assistants
RESEARCHER_ASSISTANT_ID=your-researcher-assistant-id
WRITER_ASSISTANT_ID=your-writer-assistant-id
REVIEWER_ASSISTANT_ID=your-reviewer-assistant-id

# Bing Search (Direct API) Settings
BING_SEARCH_KEY=your-bing-search-key
BING_SEARCH_ENDPOINT=https://api.bing.microsoft.com/
TAVILY_API_KEY=your-tavily-api-key

# Tracing Settings
AZURE_TRACING_GEN_AI_CONTENT_RECORDING_ENABLED=true
AZURE_SDK_TRACING_IMPLEMENTATION=opentelemetry 